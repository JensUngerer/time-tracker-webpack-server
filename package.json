  "scripts": {
    "start": "webpack --progress --env.development --env.nodemon",
    "start:prod": "webpack --progress --env.nodemon",
    "build:prod": "webpack --progress",
    "build:ci": "webpack",
    "build:dev": "webpack --env.development",
    "build:docker": "docker build --rm -t toxsickcoder/node-typescript-starter:latest .",
    "deploy:docker": "sh ./scripts/deploy.sh",
    "analyse:dev": "webpack --progress --env.development --env.analyse && start dist/report.html",
    "analyse:prod": "webpack --progress --env.analyse && start dist/report.html",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/lodash": "^4.14.165",
    "@types/mongodb": "^3.5.33",
    "@types/node": "^14.14.6",
    "@types/webpack": "^4.41.24",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "aws4": "^1.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "mongodb-client-encryption": "^1.1.0",
    "nodemon-webpack-plugin": "^4.3.2",
    "ts-loader": "^8.0.10",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
